{
    "manifest_version": 3,
    "name": "BetterMoodle",
    "version": "1.0",
    "description": "Add better functions to moodle",
    "icons": {
        "16": "images/logo-mi-16.png",
        "32": "images/logo-mi-32.png",
        "48": "images/logo-mi-48.png",
        "128": "images/logo-mi-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.moodle.aau.dk/*"
            ],
            "css": [
                "main.css"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}